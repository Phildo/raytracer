!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
alloc_px_canvas	img.cpp	/^void alloc_px_canvas(canvas *img) { img->px = (vec3 *)malloc(sizeof(vec3)*img->w*img->h); }$/;"	f
alloc_px_ppm	img.cpp	/^void alloc_px_ppm(ppm *img) { img->px = (unsigned char *)malloc(sizeof(unsigned char)*3*img->w*img->h); }$/;"	f
canvas_to_ppm	img.cpp	/^void canvas_to_ppm(canvas cimg, ppm pimg)$/;"	f
gen_img	img.cpp	/^void gen_img(canvas img)$/;"	f
main	main.cpp	/^int main(int argv, char **argc)$/;"	f
ppm_for_canvas	img.cpp	/^ppm ppm_for_canvas(canvas cimg)$/;"	f
print_ppm	img.cpp	/^void print_ppm(ppm img)$/;"	f
